<html>
  <head>
    <title>WebCampBackendNew</title>
  </head>
  <body>
    <div class="index">
      <h1>Books</h1>

      <h2>New book</h2>

      <div class="create-form">
      <%= form_with model: @book do |f| %>
        <h4>Title</h4>
          <%= f.text_field :title %>

        <h4>Body</h4>
          <%= f.text_area :body %>

          <%= f.submit :'Create Book'%>
      <% end %>
      </div>
    </div>
  </body>
</html>
